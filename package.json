    "normalize.css": "^8.0.1",
    "prop-types": "^15.7.2",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "styled-components": "^5.2.1",
    "tinycolor2": "^1.4.2"
  },
  "homepage": "https://github.com/UrijHoruzij/ui-nature"
}
