{
  "name": "ui-nature",
  "version": "0.5.2",
  "scripts": {
    "eject": "react-scripts eject",
    "style": "styleguidist server",
    "build": "npm run rm && npm run scss && cross-env NODE_ENV=production babel src/components --out-dir dist && copyfiles --flat src/components/**/*.woff2 dist/Fonts",
    "scss": "node-sass --output-style compressed src/components -o dist",
    "rm": "rimraf ./dist"
  },
  "devDependencies": {
    "@babel/cli": "^7.12.1",
    "@babel/core": "^7.12.3",
    "@babel/preset-env": "^7.12.1",
    "@babel/preset-react": "^7.12.5",
    "babel-loader": "^8.2.1",
    "babel-plugin-transform-rename-import": "^2.3.0",
    "copyfiles": "^2.4.0",
    "cross-env": "^7.0.2",
    "css-loader": "^5.0.1",
    "file-loader": "^6.2.0",
    "node-sass": "^4.14.1",
    "prettier": "^2.2.1",
    "prop-types": "^15.7.2",
    "raw-loader": "^4.0.2",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-scripts": "4.0.0",
    "react-styleguidist": "^11.1.2",
    "resolve-url-loader": "^3.1.2",
    "rimraf": "^3.0.2",
    "sass": "^1.29.0",
    "sass-loader": "^10.1.0",
    "url-loader": "^4.1.1",
    "webpack": "^5.4.0"
  },
  "main": "dist/index.js",
  "module": "dist/index.js",
  "files": [
    "dist",
    "README.md"
  ],
