{
  "name": "ui-nature",
  "version": "1.0.3",
  "scripts": {
    "storybook": "start-storybook -p 6006 -s public --no-manager-cache",
    "build-storybook": "build-storybook -o docs-build",
    "eject": "react-scripts eject",
    "test": "react-scripts test",
    "build": "npm run rm && cross-env NODE_ENV=production babel src/components --out-dir dist && copyfiles --flat src/components/**/*.woff2 dist/Fonts",
    "rm": "rimraf ./dist",
    "prepare": "husky install"
  },
  "devDependencies": {
    "@babel/cli": "^7.22.5",
    "@babel/core": "^7.23.0",
    "@babel/preset-env": "^7.20.2",
    "@babel/preset-react": "^7.18.6",
    "@commitlint/cli": "^17.6.5",
    "@commitlint/config-conventional": "^17.6.6",
    "@semantic-release/changelog": "^6.0.3",
    "@semantic-release/git": "^10.0.1",
    "@storybook/addon-actions": "^7.0.23",
    "@storybook/addon-essentials": "^8.1.5",
    "@storybook/addon-links": "^7.0.18",
    "@storybook/node-logger": "^7.4.5",
    "@storybook/preset-create-react-app": "^7.0.7",
    "@storybook/react": "^7.4.5",
    "@storybook/theming": "^7.0.24",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^12.1.5",
    "@testing-library/user-event": "^14.4.3",
    "babel-plugin-transform-rename-import": "^2.3.0",
    "copyfiles": "^2.4.0",
    "cross-env": "^7.0.2",
    "husky": "^8.0.3",
    "jest-styled-components": "^7.1.1",
    "prettier": "^3.3.3",
    "prop-types": "^15.8.1",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-scripts": "5.0.1",
    "react-test-renderer": "^17.0.2",
    "rimraf": "^4.4.0",
    "storybook-addon-styled-component-theme": "^2.0.0"
  },
  "main": "dist/index.js",
  "module": "dist/index.js",
  "files": [
    "dist",
    "README.md"
  ],
  "repository": {
    "type": "git",
    "url": "ssh://git@github.com/UrijHoruzij/ui-nature.git",
    "directory": "UrijHoruzij/ui-nature"
  },
  "author": "Urij Horuzij",
  "license": "MIT",
  "dependencies": {
    "normalize.css": "^8.0.1",
    "prop-types": "^15.8.1",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "styled-components": "^5.3.10",
    "tinycolor2": "^1.6.0"
  },
  "homepage": "https://github.com/UrijHoruzij/ui-nature",
  "eslintConfig": {
    "overrides": [
      {
        "files": [
          "**/*.stories.*"
        ],
        "rules": {
          "import/no-anonymous-default-export": "off"
        }
      }
    ]
  }
}
