{
  "name": "ui-nature",
  "version": "0.7.2",
  "scripts": {
    "storybook": "start-storybook -p 6006 -s public --no-manager-cache",
    "build-storybook": "build-storybook -o docs-build",
    "eject": "react-scripts eject",
    "style": "styleguidist server",
    "test": "react-scripts test",
    "build": "npm run rm && npm run scss && cross-env NODE_ENV=production babel src/components --out-dir dist && copyfiles --flat src/components/**/*.woff2 dist/Fonts",
    "scss": "node-sass --output-style compressed src/components -o dist",
    "rm": "rimraf ./dist",
    "prepare": "husky install"
  },
  "devDependencies": {
    "@babel/cli": "^7.12.1",
    "@babel/core": "^7.12.3",
    "@babel/preset-env": "^7.12.1",
    "@babel/preset-react": "^7.12.5",
    "@commitlint/cli": "^17.0.2",
    "@commitlint/config-conventional": "^17.0.2",
    "@storybook/addon-actions": "^6.4.9",
    "@storybook/addon-essentials": "^6.4.9",
    "@storybook/addon-links": "^6.4.9",
    "@storybook/node-logger": "^6.4.9",
    "@storybook/preset-create-react-app": "^3.2.0",
    "@storybook/react": "^6.4.9",
    "@storybook/theming": "^6.4.9",
    "@testing-library/jest-dom": "^5.16.1",
    "@testing-library/react": "^12.1.2",
    "@testing-library/user-event": "^13.5.0",
    "babel-plugin-transform-rename-import": "^2.3.0",
    "copyfiles": "^2.4.0",
    "cross-env": "^7.0.2",
    "husky": "^8.0.1",
    "jest-styled-components": "^7.0.8",
    "node-sass": "^7.0.0",
    "prettier": "^2.2.1",
    "prop-types": "^15.7.2",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-scripts": "4.0.0",
    "react-test-renderer": "^17.0.2",
    "rimraf": "^3.0.2",
    "sass": "^1.29.0",
    "storybook-addon-styled-component-theme": "^2.0.0"
  },
  "main": "dist/index.js",
  "module": "dist/index.js",
  "files": [
    "dist",
    "README.md"
  ],
  "repository": {
    "type": "git",
    "url": "ssh://git@github.com/UrijHoruzij/ui-nature.git",
    "directory": "UrijHoruzij/ui-nature"
  },
  "author": "Urij Horuzij",
  "license": "MIT",
  "dependencies": {
    "normalize.css": "^8.0.1",
    "prop-types": "^15.7.2",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "styled-components": "^5.2.1",
    "tinycolor2": "^1.4.2"
  },
  "homepage": "https://github.com/UrijHoruzij/ui-nature",
  "eslintConfig": {
    "overrides": [
      {
        "files": [
          "**/*.stories.*"
        ],
        "rules": {
          "import/no-anonymous-default-export": "off"
        }
      }
    ]
  }
}
